use online_shopping_db;


 Admin_login Table :-
 create table admin_login (id int not null auto_increment primary key, username varchar(255), password varchar(255));
 insert into admin_login values (1,"Akshay","12345");


Product_data :-
create table product_data (id int auto_increment primary key, name varchar(255), price int, quantity int);
insert into product_data (name,price,quantity) values ("Desktop", "10000", "20");